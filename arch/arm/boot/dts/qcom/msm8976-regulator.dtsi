/* SPM controlled regulators */
&spmi_bus {
	qcom,pm8950@1 {
		/* PM8950 S5 VDD_APC0 supply */
		pm8950_s5: spm-regulator@2000 {
			compatible = "qcom,spm-regulator";
			reg = <0x2000 0x100>;
			regulator-name = "pm8950_s5";
			regulator-min-microvolt = <500000>;
			regulator-max-microvolt = <1085000>;
			/* If SPM doesn't work bypass-spm */
			/* qcom,bypass-spm */
		};
	};

	qcom,pm8004@1 {
		/* PM8004 S2-S4 VDD_APC1 supply */
		pm8950_s2: spm-regulator@1700 {
			compatible = "qcom,spm-regulator";
			reg = <0x1700 0x100>;
			regulator-name = "pm8950_s2";
			regulator-min-microvolt = <500000>;
			regulator-max-microvolt = <1085000>;
			/* If SPM doesn't work bypass-spm */
			/* qcom,bypass-spm */
		};

		/* PM8004 S5 = VDD_GFX supply */
		pm8004_s5: spm-regulator@2000 {
			compatible = "qcom,qpnp-regulator";
			reg = <0x2000 0x100>;
			regulator-name = "pm8004_s5";
			regulator-min-microvolt = <500000>;
			regulator-max-microvolt = <1050000>;
		};
	};
};

&soc {
	apc0_vreg_corner: regulator-apc0-corner {
		compatible = "qcom,stub-regulator";
		regulator-name = "apc0_corner";
		regulator-min-microvolt = <1>;
		regulator-max-microvolt = <3>;
	};

	apc1_vreg_corner: regulator-apc1-corner {
		compatible = "qcom,stub-regulator";
		regulator-name = "apc1_corner";
		regulator-min-microvolt = <1>;
		regulator-max-microvolt = <3>;
	};

	gfx_vreg_corner: regulator-gfx-corner {
		compatible = "qcom,stub-regulator";
		regulator-name = "gfx_corner";
		regulator-min-microvolt = <1>;
		regulator-max-microvolt = <3>;
	};
};

&pmi8950_charger {
	smbcharger_charger_otg: qcom,smbcharger-boost-otg {
		regulator-name = "smbcharger_charger_otg";
	};
};

&soc {
	eldo2_8976: eldo2 {
		compatible = "regulator-fixed";
		regulator-name = "eldo2_8976";
		startup-delay-us = <0>;
		enable-active-high;
		gpio = <&pm8950_gpios 7 0>;
	};
};
