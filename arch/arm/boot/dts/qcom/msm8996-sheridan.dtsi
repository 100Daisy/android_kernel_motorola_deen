#include "msm8996-v3.dtsi"
#include "msm8996-moto-common.dtsi"
#include "dsi-panel-mot-smd-546-QHD-g-cmd.dtsi"

/ {
	model = "Sheridan";
	compatible = "qcom,msm8996-sheridan", "qcom,msm8996-moto",
			"qcom,msm8996";
};

&drv2605 {
	effects_library = <6>;
	rated_voltage = <0x2d>;         /* 0.96v */
	overdrive_voltage = <0x89>;     /* 3v */
	rtp_overdrive_voltage = <0x89>; /* 3v */
	lra_drive;
};

/{
	mot_batterydata: qcom,battery-data {
		qcom,batt-id-range-pct = <100>;
		df-serialnum = "SNN5972";

		#include "batterydata-gv30-lg.dtsi"
	};
};

&pmi8994_fg {
	/delete-property/ qcom,ext-sense-type; /* External Sense Resistor */
	qcom,battery-data = <&mot_batterydata>;
};

&soc {
	vbat_disp_vreg: vbat_disp_vreg {
		compatible = "regulator-fixed";
		regulator-name = "vbat_disp";
		gpio = <&tlmm 69 0>;
		startup-delay-us = <300>;
		enable-active-high;
		regulator-boot-on;
	};

	vdd3_disp_vreg: vdd3_disp_vreg {
		compatible = "regulator-fixed";
		regulator-name = "vdd3_disp";
		gpio = <&pm8994_gpios 5 0>;
		vin-supply = <&pm8994_s4>;
		startup-delay-us = <300>;
		enable-active-high;
		regulator-boot-on;
	};

	qcom,msm-thermal {
		qcom,config-info = "sheridan";
	};

	/* SensorHub Bus */
	i2c@7579000 {
		motosh@43 {
			mag_config = <4>;     /* Magnetometer config value */
		};
	};
};

&pm8994_l11 {
	regulator-min-microvolt = <1100000>;
	regulator-max-microvolt = <1300000>;
	qcom,init-voltage = <1250000>;
};

&pm8994_l17 {
	regulator-always-on;
	regulator-min-microvolt = <3000000>;
	regulator-max-microvolt = <3000000>;
	qcom,init-voltage = <3000000>;
};

&pm8994_l19 {
	regulator-boot-on;
	regulator-min-microvolt = <3100000>;
	regulator-max-microvolt = <3100000>;
	qcom,init-voltage = <3100000>;
};

&mdss_mdp {
	mot_tdi_546_1080p_cmd_v0_secondary: qcom,mdss_dsi_mot_tdi_546_1080p_cmd_v0_secondary {
		#include "dsi-panel-mot-tdi-546-1080p-cmd-common.dtsi"
	};
};

&mdss_dsi {
	hw-config = "dual_dsi";
};

&mdss_dsi0 {
	qcom,dsi-pref-prim-pan = <&mot_smd_546_QHD_g_cmd_v0>;
	vbat_disp-supply = <&vbat_disp_vreg>;
	vci_disp-supply = <&pm8994_l19>;
	vdd3_disp-supply = <&vdd3_disp_vreg>;
};

&mdss_dsi1 {
	qcom,dsi-pref-prim-pan = <&mot_tdi_546_1080p_cmd_v0_secondary>;
	apba_cdsi-supply = <&pm8994_l11>;
};

&mot_smd_546_QHD_g_cmd_v0 {
	qcom,cont-splash-enabled;
};

&mot_tdi_546_1080p_cmd_v0_secondary {
	qcom,mdss-dsi-panel-name = "mipi_mot_cmd_tdi_1080p_546_v0_secondary";
	qcom,mdss-tear-check-disable;
	qcom,mdss-dsi-no-panel-read-support;
	qcom,mdss-dsi-bl-pmic-control-type = "bl_ctrl_mot";
	/delete-property/ qcom,mdss-dsi-on-command;
	/delete-property/ qcom,mdss-dsi-off-command;
	/delete-property/ qcom,mdss-dsi-te-pin-select;
	/delete-property/ qcom,mdss-dsi-te-dcs-command;
	/delete-property/ qcom,mdss-dsi-te-check-enable;
	/delete-property/ qcom,mdss-dsi-te-using-te-pin;
	/delete-node/ qcom,panel-supply-entries;

	/* These power supplies are for DBA panel */
	qcom,panel-supply-entries {
		#address-cells = <1>;
		#size-cells = <0>;

		qcom,panel-supply-entry@0 {
			reg = <0>;
			qcom,supply-name = "apba_cdsi";
			qcom,supply-min-voltage = <1100000>;
			qcom,supply-max-voltage = <1300000>;
			qcom,supply-enable-load = <300000>;
			qcom,supply-disable-load = <100>;
			qcom,supply-pre-on-sleep = <0>;
			qcom,supply-post-on-sleep = <1>;
			qcom,supply-pre-off-sleep = <0>;
			qcom,supply-post-off-sleep = <1>;
		};
	};
};

&i2c_7 {
	stmvl53l0@29 {
		st,xtalkval = <22>;
	};
};

&florida {
	adsps {
		mot,speaker-protect-coeff = "speaker-protect-sheridan";
	};
};
